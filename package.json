{
  "name": "lobbywatch-frontend",
  "version": "1.0.0",
  "engines": {
    "node": "14.4.0"
  },
  "description": "Lobbywatch.ch Frontend",
  "scripts": {
    "dev": "nodemon -w next.config.js -w jsconfig.json --exec 'next dev'",
    "build": "next build",
    "start": "next start -p ${PORT:=3000}",
    "test": "standard 'src/**/*.js' 'pages/**/*.js'",
    "standardize": "standard --fix 'src/**/*.js' 'pages/**/*.js'",
    "translations": "gsheets --key=1FhjogYL2SBxaJG3RfR01A7lWtb3XTE2dH8EtYdmdWXg --title=translations --pretty --out lib/translations.json",
    "klog": "find . -name '*.js' -not -path \"./node_modules*\" -not -path \"./.next*\" | xargs wc -l"
  },
  "author": "Thomas Preusse <t@preus.se> (https://t.preus.se/)",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lobbywatch/website.git"
  },
  "bugs": {
    "url": "https://github.com/lobbywatch/website/issues"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "accept-language": "^3.0.15",
    "apollo-server-micro": "^2.23.0",
    "core-js": "^3.10.1",
    "d3-array": "^1.0.2",
    "d3-collection": "^1.0.2",
    "d3-format": "^1.4.5",
    "d3-hierarchy": "^1.1.9",
    "d3-time": "^1.1.0",
    "d3-time-format": "^2.3.0",
    "dataloader": "^1.3.0",
    "entities": "^2.2.0",
    "glamor": "^2.20.22",
    "graphql": "^15.5.0",
    "graphql-tools": "^4.0.8",
    "gsheets": "^2.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "lru-cache": "^5.1.1",
    "next": "^10.1.3",
    "prop-types": "^15.6.0",
    "querystring": "^0.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "striptags": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "nodemon": "^2.0.7",
    "standard": "^14.3.4"
  },
  "cacheDirectories": [
    "node_modules",
    ".next/cache"
  ]
}
