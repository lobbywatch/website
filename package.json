{
  "name": "lobbywatch-frontend",
  "version": "1.0.0",
  "engines": {
    "node": ">=22"
  },
  "description": "Lobbywatch.ch Frontend",
  "scripts": {
    "fmt": "prettier --write .",
    "dev": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start -p ${PORT:=3000}",
    "prepare": "husky install",
    "lint": "eslint --fix .",
    "test": "eslint",
    "klog": "find . -name '*.js' -not -path \"./node_modules*\" -not -path \"./.next*\" | xargs wc -l"
  },
  "author": "Thomas Preusse <t@preus.se> (https://t.preus.se/)",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lobbywatch/website.git"
  },
  "bugs": {
    "url": "https://github.com/lobbywatch/website/issues"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "@apollo/client": "^3.5.10",
    "accept-language": "^3.0.15",
    "clipboard-copy": "^4.0.1",
    "d3-array": "^1.0.2",
    "d3-collection": "^1.0.2",
    "d3-ease": "^1.0.7",
    "d3-format": "^1.4.5",
    "d3-hierarchy": "^1.1.9",
    "d3-interpolate": "^3.0.1",
    "d3-time": "^1.1.0",
    "d3-time-format": "^2.3.0",
    "d3-timer": "^1.0.10",
    "express": "^4.21.2",
    "glamor": "^2.20.22",
    "graphql": "^16.8.1",
    "next": "^14.2.31",
    "querystring": "^0.2.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-maskedinput": "^4.0.1",
    "react-textarea-autosize": "^7.1.2",
    "swr": "2.3.3"
  },
  "devDependencies": {
    "@types/node": "^20.14.2",
    "@types/react": "^18.3.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^14.2.3",
    "eslint-config-prettier": "^9.1.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.5",
    "prettier": "3.6.2",
    "typescript": "5.9.2"
  },
  "cacheDirectories": [
    "node_modules",
    ".next/cache"
  ]
}
