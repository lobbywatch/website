{
  "name": "lobbywatch-frontend",
  "version": "1.0.0",
  "engines": {
    "node": "14.4.0"
  },
  "description": "Lobbywatch.ch Frontend",
  "scripts": {
    "dev": "nodemon -w next.config.js -w jsconfig.json --exec 'next dev'",
    "build": "next build",
    "start": "next start -p ${PORT:=3000}",
    "prepare": "husky install",
    "lint": "eslint --fix .",
    "test": "eslint",
    "translations": "NODE_OPTIONS='-r dotenv/config' gsheets --key=1FhjogYL2SBxaJG3RfR01A7lWtb3XTE2dH8EtYdmdWXg --title=translations --pretty --out lib/translations.json",
    "klog": "find . -name '*.js' -not -path \"./node_modules*\" -not -path \"./.next*\" | xargs wc -l"
  },
  "author": "Thomas Preusse <t@preus.se> (https://t.preus.se/)",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lobbywatch/website.git"
  },
  "bugs": {
    "url": "https://github.com/lobbywatch/website/issues"
  },
  "lint-staged": {
    "*.js": "eslint --cache --fix"
  },
  "dependencies": {
    "@apollo/client": "^3.3.18",
    "accept-language": "^3.0.15",
    "apollo-server-micro": "^2.24.0",
    "core-js": "^3.12.1",
    "d3-array": "^1.0.2",
    "d3-collection": "^1.0.2",
    "d3-format": "^1.4.5",
    "d3-hierarchy": "^1.1.9",
    "d3-time": "^1.1.0",
    "d3-time-format": "^2.3.0",
    "dataloader": "^2.0.0",
    "entities": "^2.2.0",
    "express": "^4.17.1",
    "glamor": "^2.20.22",
    "graphql": "^15.5.0",
    "graphql-tools": "^7.0.5",
    "gsheets": "^3.0.1",
    "isomorphic-unfetch": "^3.1.0",
    "lru-cache": "^6.0.0",
    "next": "^10.2.2",
    "prop-types": "^15.6.0",
    "querystring": "^0.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "striptags": "^3.0.1"
  },
  "devDependencies": {
    "dotenv": "^10.0.0",
    "eslint": "^7.26.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-sonarjs": "^0.7.0",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "nodemon": "^2.0.7",
    "prettier": "^2.3.0"
  },
  "cacheDirectories": [
    "node_modules",
    ".next/cache"
  ]
}
