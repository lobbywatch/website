{
  "name": "lobbywatch-frontend",
  "version": "1.0.0",
  "engines": {
    "node": "14.4.0"
  },
  "description": "Lobbywatch.ch Frontend",
  "scripts": {
    "dev": "nodemon -w server.js -w routes.js -w constants.js -w graphql server.js",
    "build": "next build",
    "start": "NODE_ENV=production node server.js",
    "test": "standard 'src/**/*.js' 'pages/**/*.js'",
    "standardize": "standard --fix 'src/**/*.js' 'pages/**/*.js'",
    "translations": "gsheets --key=1FhjogYL2SBxaJG3RfR01A7lWtb3XTE2dH8EtYdmdWXg --title=translations --pretty --out lib/translations.json",
    "klog": "find . -name '*.js' -not -path \"./node_modules*\" -not -path \"./.next*\" | xargs wc -l"
  },
  "author": "Thomas Preusse <t@preus.se> (https://t.preus.se/)",
  "license": "BSD-3-Clause",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/lobbywatch/website.git"
  },
  "bugs": {
    "url": "https://github.com/lobbywatch/website/issues"
  },
  "standard": {
    "parser": "babel-eslint"
  },
  "dependencies": {
    "accept-language": "^3.0.15",
    "apollo-cache-inmemory": "^1.6.6",
    "apollo-client": "^2.6.10",
    "apollo-link": "^1.2.14",
    "apollo-link-http": "^1.5.17",
    "apollo-server": "^2.19.0",
    "apollo-server-express": "^2.19.0",
    "core-js": "^3.7.0",
    "cors": "^2.8.1",
    "d3-array": "^1.0.2",
    "d3-collection": "^1.0.2",
    "d3-format": "^1.4.5",
    "d3-hierarchy": "^1.1.9",
    "d3-time": "^1.1.0",
    "d3-time-format": "^2.3.0",
    "dataloader": "^1.3.0",
    "entities": "^2.1.0",
    "express": "^4.17.1",
    "glamor": "^2.20.22",
    "graphql": "^14.7.0",
    "graphql-playground-middleware-express": "^1.7.22",
    "graphql-tag": "^2.11.0",
    "gsheets": "^2.0.0",
    "isomorphic-unfetch": "^3.1.0",
    "lru-cache": "^5.1.1",
    "next": "^10.0.1",
    "next-routes": "^1.4.2",
    "prop-types": "^15.6.0",
    "querystring": "^0.2.0",
    "react": "^17.0.1",
    "react-apollo": "^2.5.8",
    "react-dom": "^17.0.1",
    "striptags": "^3.0.1",
    "throng": "^4.0.0"
  },
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "dotenv": "^8.2.0",
    "nodemon": "^2.0.6",
    "standard": "^14.3.4"
  },
  "cacheDirectories": [
    "node_modules",
    ".next/cache"
  ]
}
